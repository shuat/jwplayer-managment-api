{
  "root": true,
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "no-console": 0,
    "no-unused-vars": [
      "error",
      {
        "args": "none"
      }
    ],
    "semi": "error",
    "strict": "error"
  }
}